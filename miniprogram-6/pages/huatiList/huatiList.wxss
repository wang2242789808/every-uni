/* pages/huatiList/huatiList.wxss */
.header {
    height: 400rpx;
    background: url(https://wap.lexuemiao.com/img/huatiHeader.91dd70bf.png) no-repeat;
    background-size: 100% 100%;
}

.header text {
    position: relative;
}

.header text::before {
    display: inline-block;
    content: '';
    width: 22rpx;
    height: 22rpx;
    border-bottom: 2px solid #ccc;
    border-left: 2px solid #ccc;
    transform: rotate(45deg);
    position: absolute;
    top: 30rpx;
    left: 30rpx;
}

.content {
    padding: 50rpx 20rpx;
}

.lb_item {
    margin: 10rpx 0;
    border-bottom: 1px solid #eee;
}

.lb_item .lb_item-top {
    display: flex;
    justify-content: space-between;
    align-items: center;
    font-size: 25rpx;
}

.lb_item .lb_item-top image {
    width: 40rpx;
    height: 40rpx;
    border-radius: 50%;
}

.lb_item .lb_item-top view {
    display: flex;
    align-items: center;
}

.lb_item .lb_item-top view text {
    margin-left: 10rpx;
}

.lb_item .lb_item-top view:nth-child(2) {
    color: #aaa;
}

.lb_item .lb_item-content {
    height: 230rpx;
    border-radius: 20rpx;
    padding: 20rpx 40rpx 20rpx 20rpx;
    box-sizing: border-box;
    margin: 30rpx 0;
}

.lb_item .lb_item-content .title {
    display: flex;
    align-items: center;
    color: #fff;
    font-size: 35rpx;
}

.lb_item .lb_item-content .title .jing {
    background: url(data:image/png;base64,) no-repeat;
    background-size: 100% 100%;
    width: 40rpx;
    height: 40rpx;
    float: left;
    margin: 0 10rpx;
}

.lb_item-content .txt {
    font-size: 26rpx;
    color: #fff;
    margin-top: 20rpx;
}

.lb_item2 {
    display: flex;
    justify-content: space-between;
    border-bottom: 1px solid #eee;
    padding: 20rpx 0;
    height: 200rpx;
}

.lb_item2 .lb_item2-left {
    width: 30%;
}

.lb_item2 .lb_item2-right {
    padding: 0 20rpx;
    flex: 1;
    font-size: 25rpx;
    position: relative;
}

.lb_item2 .lb_item2-left image {
    width: 100%;
    height: 100%;
    border-radius: 10rpx;
}

.lb_item2 .lb_item2-right .title {
    display: flex;
    font-size: 28rpx;
}

.lb_item2 .lb_item2-right .title .jing {
    background: url(data:image/png;base64,) no-repeat;
    background-size: 100% 100%;
    width: 40rpx;
    height: 40rpx;
    float: left;
    margin: 0 10rpx;
}

.lb_item2 .lb_item2-right .pl {
    color: #aaa;
    position: absolute;
    bottom: 0;
}