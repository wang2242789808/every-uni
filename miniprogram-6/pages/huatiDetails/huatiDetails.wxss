/* pages/huatiDetails/huatiDetails.wxss */
.header {
    position: relative;
    text-align: center;
    height: 50rpx;
    line-height: 50rpx;
    font-size: 36rpx;
    font-weight: 600;
}

.header text:nth-child(1)::before {
    content: '';
    display: inline-block;
    width: 22rpx;
    height: 22rpx;
    border-left: 2px solid #333;
    border-bottom: 2px solid #333;
    transform: rotate(45deg);
    position: absolute;
    left: 20rpx;
    top: 5rpx;
}

.w_bfb {
    padding: 80rpx 30rpx;
    box-sizing: border-box;
}

.imgBox {
    height: 200rpx;
    border-radius: 20rpx;
    padding: 20rpx;
    box-sizing: border-box;
}

.imgBox .title {
    display: flex;
    align-items: center;
    color: #fff;
    margin-bottom: 15rpx;
}

.imgBox .title .jing {
    background: url(data:image/png;base64,) no-repeat;
    background-size: 100% 100%;
    width: 42rpx;
    height: 42rpx;
    float: left;
    margin-right: 20rpx;
}

.imgBox .txt {
    font-size: 26rpx;
    color: #fff;
}

.bottom {
    text-align: center;
    color: #aaa;
    font-size: 25rpx;
    margin-top: 30rpx;
}

.release {
    width: 100%;
    padding: 4% 2%;
    box-sizing: border-box;
    border-top: 20rpx solid #efefef;
}

.release .release-top {
    /* height: 100rpx; */
    display: flex;
    justify-content: flex-start;
    padding: 0 40rpx;
    box-sizing: border-box;
}

.release .left {
    width: 60%;
    display: flex;
}

.release .left .name-box {
    margin-left: 20rpx;
    font-size: 28rpx;
}

.release .left .name-box .time {
    color: #999;
    margin-top: 8rpx;
}

.release .left .img {
    position: relative;
    width: 80rpx;
    height: 80rpx;
    border-radius: 50%;
}

.release .left .v {
    position: absolute;
    right: -6rpx;
    bottom: -6rpx;
    width: 26rpx;
    height: 26rpx;
}

.release .left .img .to {
    width: 100%;
    height: 100%;
    border-radius: 50%;
}

.right {
    width: 30%;
}

.release .release-top .right .guan {
    width: 120rpx;
    height: 52rpx;
    line-height: 52rpx;
    margin-top: 26rpx;
    float: right;
    color: #f15a24;
    background: rgba(241, 90, 36, .12);
    border-radius: 35rpx;
    text-align: center;
    font-size: 24rpx;
    font-weight: 500;
    margin-left: 280rpx;
}

/* 尾部 */
.footer {
    padding: 40rpx 0;
    display: flex;
    justify-content: space-around;
    border-top: 2rpx solid #eee;
}

.substance {
    padding: 40rpx;
    box-sizing: border-box;
    font-size: 32rpx;
}

.substance .substance-title {
    margin: 20rpx 0 40rpx 0;
    font-size: 25rpx;
}

.substance .substance-img {
    width: 100%;
    display: flex;
}

.substance .substance-img view {
    width: 50%;
    height: 330rpx;
    margin-right: 20rpx;
}

.substance .substance-img view image {
    width: 100%;
    height: 100%;
    border-radius: 25rpx;
}

.substance-img2 {
    width: 100%;
    display: flex;
    flex-wrap: wrap;
}

.substance-img2 view {
    width: 30%;
    height: 200rpx;
    margin: 10rpx;
    border-radius: 25rpx;
}

.substance-img2 view image {
    width: 100%;
    height: 100%;
    border-radius: 25rpx;
}

.dian {
    float: right;
    margin-left: 10rpx;
    margin-top: 45rpx;
}

.dian text {
    background: #9fa3b0;
    width: 10rpx;
    height: 10rpx;
    border-radius: 50%;
    float: left;
    margin-left: 8rpx;
}

.dian text:nth-of-type(2) {
    background: #424963;
}

.footer view {
    display: flex;
    align-items: center;
    color: #ccc;
    font-size: 30rpx;
}

.footer view image {
    width: 40rpx;
    height: 35rpx;
    margin-right: 10rpx;
}